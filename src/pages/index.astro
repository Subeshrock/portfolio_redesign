---
import Layout from "../layouts/BaseLayout.astro";
import Hero from "../components/Hero.astro";
import Header from "../components/Header.astro";
import About from "../components/About.astro";
import Skills from "../components/Skills.astro";
import Experience from "../components/Experience.astro";
import Projects from "../components/Projects.astro";
import Education from "../components/Education.astro";
import Awards from "../components/Awards.astro";
import Contact from "../components/Contact.astro";
import Footer from "../components/Footer.astro";
const seo = {
  title: "Subesh Yadav - Full-Stack Developer & UI/UX Designer | Portfolio",
  description: "Portfolio of Subesh Yadav, SDE II at iRM Cloud. Skilled in React, Next.js, TypeScript, NestJS, Go, Rust, Monorepo & Microfrontend architectures, CI/CD pipelines, and UI/UX design.",
  url: "https://subesh.netlify.app",
  image: "/images/preview.png",
  author: "Subesh Yadav",
  keywords: [
    "Subesh Yadav",
    "Full-Stack Developer",
    "React Developer",
    "Next.js Developer",
    "TypeScript Developer",
    "NestJS Developer",
    "Go Developer",
    "Rust Developer",
    "UI/UX Designer",
    "Frontend Engineer",
    "Backend Developer",
    "Software Engineer Portfolio",
    "Software Engineer"
  ]
};

---

<Layout {...seo}>
  <Header />
  <main>
    <Hero />
    <About />
    <Skills />
    <Experience />
    <Projects />
    <Education />
    <Awards />
    <Contact />
  </main>
  <Footer />
</Layout>
